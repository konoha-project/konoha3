import("Java.String");

void test() {
	assert("Hello World, hello world.".replace("world", "Japan") == "Hello World, hello Japan.");
	assert("Hello World, hello world.".replace("world", "Japan") == "Hello World, hello Japan.");
	assert("Hello World, hello world.".replace("world", "Japan") == "Hello World, hello Japan.");
}

test();
